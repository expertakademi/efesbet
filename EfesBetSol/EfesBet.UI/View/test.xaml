<Window x:Class="EfesBetGUI.View.test"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="test" Height="300" Width="300">
    <Window.Resources>
        <ResourceDictionary Source="/Theme/Generic.xaml">
        </ResourceDictionary>
    </Window.Resources>
    <Grid>

        <Grid x:Name="LayoutRoot">
            <ToggleButton Content="ToggleButton" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="65.743,80,0,0" Style="{StaticResource SlidingToggleButton}" BorderBrush="{x:Null}" Background="#FF2F63FF"/>
            <ToggleButton Content="ToggleButton" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="65.743,128,0,0" Style="{StaticResource SlidingToggleButton}" BorderBrush="{x:Null}" Background="#FF35862C"/>
            <ToggleButton Content="ToggleButton" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="65.743,180,0,0" Style="{StaticResource SlidingToggleButton}" BorderBrush="{x:Null}" Background="#FFAB7938"/>
            <ToggleButton Content="ToggleButton" VerticalAlignment="Top" Margin="271.743,80,192,0" Style="{StaticResource SlidingToggleButton}" BorderBrush="{x:Null}" Background="#FF2F63FF"  IsEnabled="False"/>
            <ToggleButton Content="ToggleButton" VerticalAlignment="Top" Margin="271.743,128,192,0" Style="{StaticResource SlidingToggleButton}" BorderBrush="{x:Null}" Background="#FF35862C"  IsEnabled="False"/>
            <ToggleButton Content="ToggleButton" VerticalAlignment="Top" Margin="271.743,180,192,0" Style="{StaticResource SlidingToggleButton}" BorderBrush="{x:Null}" Background="#FFAB7938"  IsEnabled="False"/>
        </Grid>
    </Grid>
</Window>
